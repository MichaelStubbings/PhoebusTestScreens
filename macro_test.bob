<?xml version="1.0" encoding="UTF-8"?>
<display version="2.0.0">
  <name>Display</name>
  <widget type="label" version="2.0.0">
    <name>Label</name>
    <class>TITLE</class>
    <text>This is a page about $(OBJECT)</text>
    <x use_class="true">0</x>
    <y use_class="true">0</y>
    <width>550</width>
    <height>31</height>
    <font use_class="true">
      <font name="Header 1" family="Liberation Sans" style="BOLD" size="22.0">
      </font>
    </font>
    <foreground_color use_class="true">
      <color name="Text" red="0" green="0" blue="0">
      </color>
    </foreground_color>
    <transparent use_class="true">true</transparent>
  </widget>
  <widget type="label" version="2.0.0">
    <name>Label_3</name>
    <text>Shape:</text>
    <y>50</y>
  </widget>
  <widget type="label" version="2.0.0">
    <name>Label_4</name>
    <text>Colour:</text>
    <y>140</y>
  </widget>
  <widget type="label" version="2.0.0">
    <name>Label_5</name>
    <text>Key Vitamins:</text>
    <y>80</y>
    <width>110</width>
  </widget>
  <widget type="label" version="2.0.0">
    <name>Label_6</name>
    <text>Origin:</text>
    <y>110</y>
  </widget>
  <widget type="led" version="2.0.0">
    <name>LED</name>
    <x>110</x>
    <y>140</y>
    <off_color>
      <color name="COLOUR" red=$(RED) green=$(GREEN) blue=$(BLUE)>
      </color>
    </off_color>
  </widget>
  <widget type="label" version="2.0.0">
    <name>Label_9</name>
    <text>$(SHAPE)</text>
    <x>110</x>
    <y>50</y>
    <background_color>
      <color name="Button_Background" red="210" green="210" blue="210">
      </color>
    </background_color>
    <transparent>false</transparent>
  </widget>
  <widget type="label" version="2.0.0">
    <name>Label_10</name>
    <text>$(VITAMINS)</text>
    <x>110</x>
    <y>80</y>
    <background_color>
      <color name="Button_Background" red="210" green="210" blue="210">
      </color>
    </background_color>
    <transparent>false</transparent>
  </widget>
  <widget type="label" version="2.0.0">
    <name>Label_11</name>
    <text>$(ORIGIN)</text>
    <x>110</x>
    <y>110</y>
    <background_color>
      <color name="Button_Background" red="210" green="210" blue="210">
      </color>
    </background_color>
    <transparent>false</transparent>
  </widget>
</display>
